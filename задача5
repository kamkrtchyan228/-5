def main():
    """
    Банкомат с жадным алгоритмом
    """
    # Номинал купюр
    denominations = [5000, 2000, 1000, 500, 200, 100]
    
    # Ввод суммы
    amount = int(input("Сумма (кратная 100): "))
    
    # Проверка
    if amount % 100 != 0:
        print("Ошибка: сумма должна быть кратной 100!")
        return
    
    # Выдача купюр
    print(f"\nВыдача {amount} руб:")
    remaining = amount
    
    for denomination in denominations:
        if remaining >= denomination:
            count = remaining // denomination
            remaining -= count * denomination
            print(f"B{denomination}: {count} шт")
    
    if remaining > 0:
        print(f"Остаток не выдан: {remaining} руб")

# Запуск
main()
